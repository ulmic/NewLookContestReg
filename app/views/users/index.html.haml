- if session[:user_id].nil?
  = render 'page_404'
- else
  %table
  %tr
    %th Login
    %th Password
    %th
    %th
    %th

  - @users.each do |admin|
    %tr
      %td= admin.first_name
      %td= admin.last_name
      %td= link_to 'Show', admin
      %td= link_to 'Edit', edit_admin_path(admin)
      %td= link_to 'Destroy', admin, method: :delete, data: { confirm: 'Are you sure?' }

  %br
